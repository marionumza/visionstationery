				data-vp-repeat="false" data-vp-offset="100">
				<img class="img img-responsive oe_big_img" src="/sync_uppercrust_theme/static/src/img/big_picture.jpg"
					alt="Odoo CMS - a big picture" />
			</div>
		</template>

		<!-- Content > Share -->
		<template id="s_share_inherit" inherit_id="website.s_share" name="Share">
			<xpath expr="//div[@class='oe_share s_share']" position="replace">
	    		<div class="oe_share s_share">
					<h3 class="mb32">Follow us</h3>
					<h3 class="oe_cu_s_share">
						<a target="_Blank" class="oe_share_facebook"
							href="https://www.facebook.com/sharer/sharer.php?u={url}">
							<i class="fa fa-facebook"></i>
						</a>
						<a target="_Blank" class="oe_share_twitter"
							href="https://twitter.com/intent/tweet?text={title}&amp;url={url}">
							<i class="fa fa-twitter"></i>
						</a>
						<a target="_Blank" class="oe_share_linkedin"
							href="http://www.linkedin.com/shareArticle?mini=true&amp;url={url}&amp;title={title}&amp;">
							<i class="fa fa-linkedin"></i>
						</a>
						<a target="_Blank" class="oe_share_google"
							href="https://plus.google.com/share?url={url}">
							<i class="fa fa-google-plus"></i>
						</a>
						<a href="mailto:?body={url}&amp;subject={title}">
							<i class="fa fa-envelope-o"></i>
						</a>
					</h3>
	    		</div>
	    	</xpath>
    	</template>

    	<!-- Content > Progressbar -->
		<template id="s_progressbar" name="Progressbar">
			<div class="oe_snippet_body s_progressbar st-progressbar">
