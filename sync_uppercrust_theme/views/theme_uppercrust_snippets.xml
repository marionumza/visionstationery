			</xpath>
			<xpath expr="//t[@t-snippet='website.s_title']" position="replace">
				<t t-snippet="website.s_title" t-thumbnail="/sync_uppercrust_theme/static/src/img/icon/block_title.png"/>
			</xpath>
			<xpath expr="//t[@t-snippet='website.s_well']" position="replace">
				<t t-snippet="website.s_well" t-thumbnail="/sync_uppercrust_theme/static/src/img/icon/well.png"/>
			</xpath>
			<xpath expr="//t[@t-snippet='website.s_share']" position="replace">
				<t t-snippet="website.s_share" t-thumbnail="/sync_uppercrust_theme/static/src/img/icon/block_button.png"/>
			</xpath>
			<xpath expr="//t[@t-snippet='website.s_comparisons']" position="replace">
				<t t-snippet="website.s_comparisons" t-thumbnail="/sync_uppercrust_theme/static/src/img/icon/block_comparison.png"/>
