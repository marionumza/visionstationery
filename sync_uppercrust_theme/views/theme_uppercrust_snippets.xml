			<xpath expr="//t[@t-snippet='website.s_text_block']" position="replace">
				<t t-snippet="website.s_text_block" t-thumbnail="/sync_uppercrust_theme/static/src/img/icon/block_text_block.png"/>
			</xpath>
		</template>
		<template id="s_banner_inherit" inherit_id="website.s_banner" name="Banner">
			<xpath expr="//div[@id='myCarousel']" position="replace">
			    <div id="myCarousel" class="carousel slide s_banner oe_custom_bg master_background" data-interval="10000">
					<!-- Indicators -->
					<ol class="carousel-indicators hidden">
						<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
					</ol>
					<div class="carousel-inner">
						<div class="item active">
							<div class="container">
								<div class="row content">
									<div class="carousel-img col-xs-12 col-sm-6 col-md-5 oe_img_div post"
										data-vp-add-class="visible animated bounceInLeft"
										data-vp-remove-class="hidebefore"
										data-vp-add-ctlass-full-view="full-visible" data-vp-repeat="false"
										data-vp-offset="100">
										<img class="img-responsive oe_mobile_img"
											src="/sync_uppercrust_theme/static/src/img/phone_hr.png" alt="Banner fcandy-ltr {Synconics Image" />
									</div>
									<div
										class="carousel_text col-xs-12 col-sm-6 col-md-7 oe_txt_div post"
										data-vp-add-class="visible animated bounceInDown"
										data-vp-remove-class="hidebefore" data-vp-add-class-full-view="full-visible"
										data-vp-repeat="false" data-vp-offset="100">
										<h2>
											<span>upper</span>crust theme
										</h2>
										<p>a beautiful responsive odoo website theme for version 8.0, 9.0 and 10.0</p>
										<div class="row post o_cu_buttons" data-vp-add-class="visible animated bounceInUp"
											data-vp-remove-class="hidebefore"
											data-vp-add-class-full-view="full-visible" data-vp-repeat="false"
											data-vp-offset="100">
											<div class="col-xs-12 col-sm-6 col-md-4 oe_btn_div_odoo">
												<!-- <a href="#" class="btn st-btn-primary btn-lg mt8 oe_cu_btn">get
													it on<br/>Odoo Store</a> -->
												<img href="#" class="oe_cu_btn_odoo"
													src="/sync_uppercrust_theme/static/src/img/btn1.png" />
											</div>
											<div class="col-xs-12 col-sm-6 col-md-3 oe_btn_div">
												<!-- <a href="#" class="btn st-btn-primary btn-lg mt8 oe_cu_btn">Odoo
													App Store</a> -->
												<img href="#" class="oe_cu_btn_sync"
													src="/sync_uppercrust_theme/static/src/img/btn2.png" />
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="carousel-control left hidden" data-target="#myCarousel"
						data-slide="prev" style="width: 10%">
						<i class="fa fa-chevron-left"></i>
					</div>
					<div class="carousel-control right hidden" data-target="#myCarousel"
						data-slide="next" style="width: 10%">
						<i class="fa fa-chevron-right"></i>
					</div>
			    </div>
			</xpath>
		</template>

		<!-- Structure > Feature -->
		<template id="s_features_inherit" inherit_id="website.s_features" name="Features">
			<xpath expr="//section"	position="replace">
